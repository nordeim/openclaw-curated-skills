{
  "name": "jk-archivist-tiktok-skill",
  "version": "1.1.0",
  "description": "ClawHub bundle for deterministic TikTok intro slide + caption generation.",
  "private": true,
  "type": "module",
  "scripts": {
    "run": "node scripts/tiktok-intro-draft.mjs",
    "test:node": "node --test tests/node/*.test.mjs",
    "test:python": "python3 -m unittest discover -s tests/python -p \"test_*.py\"",
    "test": "npm run test:node && npm run test:python",
    "validate:bundle": "node scripts/validate-bundle.mjs --clean",
    "pack": "npm run validate:bundle && node scripts/pack-skill.mjs",
    "release": "node scripts/release.mjs"
  }
}
