{
  "name": "financial",
  "version": "3.0.4",
  "description": "56+ financial data tools via Yahoo Finance. Auto-routes stock prices, fundamentals, earnings, dividends, options, crypto, forex, commodities, news, and more.",
  "auto_route": true,
  "venv": "/home/openclaw/.openclaw/venv/finance",
  "requirements": "requirements.txt",
  "entry": "scripts/yfinance_ai.py",
  "invocation": "cd /home/openclaw/.openclaw/workspace/skills/financial/scripts && source /home/openclaw/.openclaw/venv/finance/bin/activate && python3 -c \"\nimport asyncio, sys\nsys.path.insert(0, '.')\nfrom yfinance_ai import Tools\nt = Tools()\nasync def main():\n    result = await t.{method}({args})\n    print(result)\nasyncio.run(main())\n\" 2>/dev/null",
  "setup": [
    "python3 -m venv /home/openclaw/.openclaw/venv/finance",
    "source /home/openclaw/.openclaw/venv/finance/bin/activate && pip install -r /home/openclaw/.openclaw/workspace/skills/financial/requirements.txt"
  ],
  "functions": {
    "get_stock_price": {
      "description": "Current price + key metrics for a stock",
      "triggers": ["price", "stock price", "current price", "share price", "how much is", "what is the price"],
      "params": {"ticker": "string"}
    },
    "get_fast_info": {
      "description": "Quick key metrics for a stock",
      "triggers": ["fast info", "quick info", "key metrics"],
      "params": {"ticker": "string"}
    },
    "get_stock_quote": {
