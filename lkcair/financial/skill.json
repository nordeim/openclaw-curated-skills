      "params": {"ticker": "string"}
    },
    "get_historical_data": {
      "description": "Historical price data for a stock",
      "triggers": ["history", "historical", "historical data", "price history", "chart"],
      "params": {"ticker": "string", "period": "string (default: 1y)", "interval": "string (default: 1d)"}
    },
    "get_historical_price_on_date": {
      "description": "Price on a specific date",
      "triggers": ["price on date", "price on", "what was the price on"],
