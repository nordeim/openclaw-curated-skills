{
  "name": "siliconflow-image-gen",
  "version": "1.0.0",
  "description": "Generate images using SiliconFlow API (FLUX.1, Stable Diffusion, etc.)",
  "author": "MaxStorm Team",
  "license": "MIT",
  "homepage": "https://github.com/maxstorm/siliconflow-image-gen",
  "keywords": ["image", "generation", "siliconflow", "flux", "stable-diffusion", "ai"],
  "trigger": {
    "patterns": [
      "生成图片",
      "生成图像",
      "文生图",
      "画一张",
      "create image",
      "generate image",
      "text to image"
    ]
  },
  "tools": ["exec"],
  "env": {
    "SILICONFLOW_API_KEY": {
      "description": "SiliconFlow API Key for image generation",
      "required": true
    }
  },
  "config": {
    "apiKey": {
      "description": "SiliconFlow API Key",
      "required": true,
      "env": "SILICONFLOW_API_KEY"
    },
    "defaultModel": {
      "description": "Default image generation model",
      "default": "black-forest-labs/FLUX.1-schnell"
    }
  },
  "files": {
    "config": ["~/.openclaw/openclaw.json"]
  },
  "models": [
    {
      "id": "black-forest-labs/FLUX.1-schnell",
      "name": "FLUX.1 Schnell",
      "description": "Fast and free image generation (recommended)"
    },
    {
      "id": "black-forest-labs/FLUX.1-dev",
      "name": "FLUX.1 Dev",
      "description": "High quality image generation"
    },
    {
      "id": "stabilityai/stable-diffusion-3-5-large",
      "name": "Stable Diffusion 3.5",
      "description": "Stable Diffusion latest version"
    }
  ]
}
