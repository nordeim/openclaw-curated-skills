{
  "name": "siliconflow-video-gen",
  "version": "1.0.0",
  "description": "Generate videos using SiliconFlow API (Wan2.2). Supports Text-to-Video and Image-to-Video.",
  "author": "MaxStorm Team",
  "license": "MIT",
  "homepage": "https://github.com/maxstorm/siliconflow-video-gen",
  "keywords": ["video", "generation", "siliconflow", "wan2.2", "text-to-video", "image-to-video"],
  "trigger": {
    "patterns": [
      "生成视频",
      "文生视频",
      "图生视频",
      "生成短视频",
      "create video",
      "generate video",
      "text to video",
      "image to video"
    ]
  },
  "tools": ["exec"],
  "env": {
    "SILICONFLOW_API_KEY": {
      "description": "SiliconFlow API Key for video generation",
      "required": true
    }
  },
  "config": {
    "apiKey": {
      "description": "SiliconFlow API Key",
      "required": true,
      "env": "SILICONFLOW_API_KEY"
    },
    "defaultModel": {
      "description": "Default video generation model",
      "default": "Wan-AI/Wan2.2-T2V-A14B"
    }
  },
  "files": {
    "config": ["~/.openclaw/openclaw.json"]
  },
  "models": [
    {
      "id": "Wan-AI/Wan2.2-T2V-A14B",
      "name": "Wan2.2 T2V",
      "description": "Text-to-Video, cinematic quality, 14B params"
    },
    {
      "id": "Wan-AI/Wan2.2-I2V-A14B",
      "name": "Wan2.2 I2V",
      "description": "Image-to-Video, cinematic quality, 14B params"
    }
  ]
}
