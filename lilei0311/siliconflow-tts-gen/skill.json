{
  "name": "siliconflow-tts-gen",
  "version": "1.0.0",
  "description": "Text-to-Speech using SiliconFlow API (CosyVoice2). Supports multiple voices, languages, and dialects.",
  "author": "MaxStorm Team",
  "license": "MIT",
  "homepage": "https://github.com/maxstorm/siliconflow-tts-gen",
  "keywords": ["tts", "text-to-speech", "siliconflow", "cosyvoice", "voice", "audio"],
  "trigger": {
    "patterns": [
      "文字转语音",
      "语音合成",
      "生成语音",
      "text to speech",
      "tts",
      "语音朗读"
    ]
  },
  "tools": ["exec"],
  "env": {
    "SILICONFLOW_API_KEY": {
      "description": "SiliconFlow API Key for TTS",
      "required": true
    }
  },
  "config": {
    "apiKey": {
      "description": "SiliconFlow API Key",
      "required": true,
      "env": "SILICONFLOW_API_KEY"
    },
    "defaultModel": {
      "description": "Default TTS model",
      "default": "FunAudioLLM/CosyVoice2-0.5B"
    },
    "defaultVoice": {
      "description": "Default voice",
      "default": "alex"
    }
  },
  "files": {
    "config": ["~/.openclaw/openclaw.json"]
  },
  "models": [
    {
      "id": "FunAudioLLM/CosyVoice2-0.5B",
      "name": "CosyVoice 2",
      "description": "Ultra-low latency (150ms), multilingual, dialect support"
    }
  ]
}
