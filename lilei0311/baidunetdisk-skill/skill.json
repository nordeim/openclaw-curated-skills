{
  "name": "baidunetdisk",
  "version": "1.0.0",
  "description": "百度网盘操作Skill - 支持文件列表查看、搜索、分享链接提取、转存、目录创建等功能",
  "author": "MaxStorm Team",
  "license": "MIT",
  "homepage": "https://github.com/maxstorm/baidunetdisk-skill",
  "keywords": ["baidu", "netdisk", "cloud", "storage", "pan", "百度网盘"],
  "trigger": {
    "patterns": [
      "百度网盘",
      "网盘",
      "baidu netdisk",
      "列出网盘",
      "搜索网盘",
      "网盘分享",
      "转存",
      "创建目录"
    ]
  },
  "tools": ["exec"],
  "entry": "scripts/main.py",
  "config": {
    "schema": {
      "bduss": {
        "type": "string",
        "description": "百度网盘登录凭证BDUSS",
        "required": true,
        "secret": true
      },
      "stoken": {
        "type": "string",
        "description": "百度网盘登录凭证STOKEN",
        "required": true,
        "secret": true
      },
      "default_save_path": {
        "type": "string",
        "description": "默认下载保存路径",
        "default": "~/Downloads/BaiduNetdisk"
      }
    }
  },
  "dependencies": {
    "python": ">=3.8",
    "packages": ["requests"]
  },
  "platforms": ["darwin", "linux"]
}
