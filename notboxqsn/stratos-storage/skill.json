{
  "name": "stratos-storage",
  "description": "Upload and download files to Stratos Decentralized Storage",
  "env": {
    "STRATOS_SPFS_GATEWAY": "http://localhost:18452",
    "STRATOS_RPC_PORT": "18252",
    "STRATOS_NODE_DIR": "~/rsnode"
  },
  "dependencies": ["curl"],
  "install": {
    "darwin": { "command": "brew install curl" },
    "linux": { "command": "apt-get install -y curl" }
  }
}
