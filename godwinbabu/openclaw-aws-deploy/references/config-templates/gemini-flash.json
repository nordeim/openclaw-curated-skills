{
  "meta": {
    "lastTouchedVersion": "2026.2.12"
  },
  "gateway": {
    "mode": "local",
    "bind": "loopback",
    "port": 18789,
    "auth": {
      "mode": "token",
      "token": "GATEWAY_TOKEN_PLACEHOLDER"
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "google/gemini-2.0-flash"
      },
      "workspace": "/home/openclaw/.openclaw/workspace",
      "heartbeat": {
        "every": "30m",
        "prompt": "Check HEARTBEAT.md if it exists. If nothing needs attention, reply HEARTBEAT_OK."
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist",
      "streamMode": "partial",
      "accounts": {
        "default": {
          "name": "AGENT_NAME_PLACEHOLDER",
          "dmPolicy": "pairing",
          "botToken": "TELEGRAM_BOT_TOKEN_PLACEHOLDER",
          "groupPolicy": "allowlist",
          "streamMode": "partial"
        }
      }
    }
  },
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      }
    }
  },
  "tools": {
    "agentToAgent": {
      "enabled": true
    }
  }
}
