{
  "name": "clawdiscover",
  "version": "1.0.0",
  "description": "Agent-centric service discovery. Discover new tools and services pushed TO your agent, not directories for humans to browse.",
  "author": "berthabot",
  "repository": "https://github.com/clawdbot/clawdiscover-skill",
  "keywords": [
    "discovery",
    "services",
    "tools",
    "x402",
    "directory",
    "agent-to-agent"
  ],
  "categories": [
    "infrastructure",
    "discovery",
    "automation"
  ],
  "triggers": [
    "discover",
    "find services",
    "new tools",
    "service discovery",
    "agent directory"
  ],
  "requires": [],
  "env": [],
  "endpoints": {
    "base": "https://clawdiscover.com",
    "free": [
      "GET /api/v1/services",
      "GET /api/v1/services/:id",
      "GET /api/v1/categories",
      "POST /api/v1/services"
    ],
    "paid": [
      {
        "endpoint": "GET /api/v1/discover",
        "price": "$0.001",
        "protocol": "x402"
      },
      {
        "endpoint": "GET /api/v1/feed",
        "price": "$0.002",
        "protocol": "x402"
      },
      {
        "endpoint": "POST /api/v1/subscribe",
        "price": "$0.01",
        "protocol": "x402"
      }
    ]
  },
  "heartbeat": {
    "recommended": true,
    "interval": "6h",
    "check": "GET /api/v1/services?sort=newest&limit=10"
  }
}
