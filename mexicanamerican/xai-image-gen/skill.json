{
  "name": "xai-image-gen",
  "version": "1.0.0",
  "description": "Generate images using xAI's Grok API (grok-imagine-image model)",
  "author": "OpenClaw Subagent",
  "license": "MIT",
  "main": "xai-gen",
  "requirements": [
    "requests>=2.28.0"
  ],
  "env": {
    "XAI_API_KEY": {
      "required": true,
      "description": "Your xAI API key from https://console.x.ai"
    }
  },
  "commands": {
    "xai-gen": {
      "description": "Generate images from text prompts",
      "usage": "xai-gen <prompt> [options]",
      "examples": [
        "xai-gen \"sunset over mountains\"",
        "xai-gen \"cyberpunk city\" --filename city.png",
        "xai-gen \"abstract art\" --n 3"
      ]
    }
  },
  "tags": ["image", "generation", "ai", "xai", "grok", "api"],
  "platform": {
    "os": ["linux", "macos", "windows"],
    "arch": ["any"],
    "python": ">=3.7"
  }
}
