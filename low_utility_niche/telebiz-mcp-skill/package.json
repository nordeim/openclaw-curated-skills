{
  "name": "telebiz-mcp",
  "version": "1.0.0",
  "description": "MCP integration for Telegram via telebiz-tt browser client",
  "type": "module",
  "main": "dist/mcp-server.js",
  "bin": {
    "telebiz-health": "dist/health.js",
    "telebiz-monitor": "dist/monitor.js"
  },
  "scripts": {
    "build": "tsc",
    "relay": "node dist/relay.js",
    "mcp": "node dist/mcp-server.js",
    "health": "node dist/health.js",
    "monitor": "node dist/monitor.js",
    "dev:relay": "tsx src/relay.ts",
    "dev:mcp": "tsx src/mcp-server.ts",
    "test": "npm run health"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
