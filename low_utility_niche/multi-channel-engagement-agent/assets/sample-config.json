{
  "platforms": {
    "twitter": {
      "enabled": true,
      "oauth": {
        "consumerKey": "YOUR_CONSUMER_KEY",
        "consumerSecret": "YOUR_CONSUMER_SECRET",
        "accessToken": "YOUR_ACCESS_TOKEN",
        "accessTokenSecret": "YOUR_ACCESS_TOKEN_SECRET"
      }
    },
    "farcaster": {
      "enabled": true,
      "neynarApiKey": "YOUR_NEYNAR_API_KEY",
      "fid": 123456,
      "signerPrivateKey": "YOUR_SIGNER_PRIVATE_KEY_HEX_NO_0X",
      "custodyPrivateKey": "0xYOUR_CUSTODY_PRIVATE_KEY"
    },
    "moltbook": {
      "enabled": true,
      "apiKey": "YOUR_MOLTBOOK_API_KEY"
    }
  },
  "persona": {
    "name": "YourAgentName",
    "bio": "Your agent's identity and purpose",
    "tone": "crypto-native, authentic, helpful",
    "signatureEmoji": "ðŸ¤–",
    "values": ["community", "building", "shipping", "helping-newcomers"],
    "avoidWords": ["excited to announce", "thrilled to share", "love this", "great post"],
    "phrases": ["ships > talks", "ser", "wagmi", "based", "gm"],
    "toneBalance": {
      "educational": 60,
      "communityVibes": 25,
      "humor": 15
    }
  },
  "filters": {
    "skipKeywords": ["airdrop", "free money", "send dm", "check bio", "dm me"],
    "minEngagement": {
      "likes": 5,
      "replies": 2
    },
    "skipBots": true,
    "languageFilter": ["en"]
  },
  "users": {
    "blacklist": [],
    "whitelist": [],
    "prioritizeVerified": true,
    "prioritizeBuilders": true
  },
  "webhooks": {
    "telegram": {
      "enabled": false,
      "botToken": "YOUR_BOT_TOKEN",
      "chatId": "YOUR_CHAT_ID"
    },
    "discord": {
      "enabled": false,
      "webhookUrl": "https://discord.com/api/webhooks/..."
    }
  },
  "settings": {
    "maxRepliesPerRun": 1,
    "trendingLimit": 5,
    "stateFile": "engagement-state.json",
    "analyticsFile": "analytics.json",
    "enableQuotes": false,
    "enableMentionTracking": true
  }
