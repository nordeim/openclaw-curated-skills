{
  "name": "funky-fund-flamingo",
  "version": "1.0.35",
  "description": "Funky Fund Flamingo is a local-only, repair-first evolution engine for OpenClaw that analyzes real session logs, memory, and installed skills, then generates prioritized, measurable mutation directives with persistent learning and paced loop progression.",
  "main": "index.js",
  "scripts": {
    "run": "node index.js run",
    "review": "node index.js run --review",
    "dry-run": "node index.js run --dry-run",
    "loop": "node index.js --loop",
    "help": "node index.js --help",
    "audit:security": "node security_audit.js",
    "audit:production": "node production_gate.js"
  },
  "keywords": [
    "openclaw",
    "funky-fund-flamingo",
    "autonomous-agent",
    "self-evolution",
    "repair-first",
    "mutation-engine",
    "log-driven",
    "resilience",
    "self-repair",
    "workflow-automation",
    "funky-fund-flamingo-mode"
  ],
  "author": "OpenClaw",
  "license": "MIT",
  "repository": { "type": "git", "url": "https://github.com/openclaw/workspace.git" },
  "homepage": "https://clawhub.ai",
  "dependencies": {},
  "openclaw": {
    "skill": true,
    "triggers": ["evolve", "evolution", "mutation", "repair", "funky fund flamingo", "self-improvement"],
    "capabilities": ["self-evolution", "log-audit", "mutation-cycles", "repair-first"],
    "files": ["SKILL.md", "README.md", "index.js", "evolve.js", "ADL.md", "VFM.md", "TREE.md", "agents/*.yaml"]
  }
}
