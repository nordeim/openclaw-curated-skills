{
  "name": "@circle/openclaw-wallet-skill",
  "version": "1.1.0",
  "description": "Circle Developer-Controlled Wallets skill for OpenClaw agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "circle-wallet": "./dist/cli.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "postinstall": "npm run build",
    "dev": "ts-node src/cli.ts",
    "test": "echo \"Tests coming soon\""
  },
  "keywords": [
    "openclaw",
    "circle",
    "usdc",
    "wallet",
    "blockchain",
    "web3"
  ],
  "author": "eltontay",
  "license": "MIT",
  "dependencies": {
    "@circle-fin/developer-controlled-wallets": "^10.1.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eltontay/clawhub_circle_wallet_skill.git"
  },
  "openclaw": {
    "type": "skill",
    "commands": [
      "circle-wallet:setup",
      "circle-wallet:config",
      "circle-wallet:create",
      "circle-wallet:balance",
      "circle-wallet:list",
      "circle-wallet:send",
      "circle-wallet:drip"
    ],
    "permissions": {
      "filesystem": ["~/.openclaw/circle-wallet/"],
      "network": ["api.circle.com"]
    }
  }
}
