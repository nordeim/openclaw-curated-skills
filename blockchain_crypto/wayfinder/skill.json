{
  "name": "wayfinder",
  "version": "0.4.0",
  "sdk_version": "main",
  "description": "DeFi trading, yield strategies, and portfolio management via Wayfinder Paths CLI. Supports token swaps, perps, prediction markets, automated yield strategies, wallet management, and protocol adapters across Ethereum, Base, Arbitrum, Polygon, BSC, Avalanche, Plasma, and HyperEVM.",
  "author": "Wayfinder Foundation",
  "entry": "SKILL.md",
  "runtime": "python",
  "commands": [
    "resource",
    "quote_swap",
    "execute",
    "hyperliquid",
    "hyperliquid_execute",
    "polymarket",
    "polymarket_execute",
    "run_strategy",
    "wallets",
    "run_script"
  ],
  "resources": {
    "static": [
      "wayfinder://adapters",
      "wayfinder://hyperliquid/markets",
      "wayfinder://hyperliquid/prices",
      "wayfinder://hyperliquid/spot-assets",
      "wayfinder://strategies",
      "wayfinder://wallets"
    ],
    "templates": [
      "wayfinder://activity/{label}",
      "wayfinder://adapters/{name}",
      "wayfinder://balances/{label}",
      "wayfinder://hyperliquid/book/{coin}",
      "wayfinder://hyperliquid/prices/{coin}",
      "wayfinder://hyperliquid/{label}/spot",
      "wayfinder://hyperliquid/{label}/state",
      "wayfinder://strategies/{name}",
      "wayfinder://tokens/gas/{chain_code}",
      "wayfinder://tokens/resolve/{query}",
      "wayfinder://tokens/search/{chain_code}/{query}",
      "wayfinder://wallets/{label}"
    ]
  },
  "references": [
    "references/setup.md",
    "references/strategies.md",
    "references/adapters.md",
    "references/coding-interface.md",
    "references/hyperliquid.md",
    "references/polymarket.md",
    "references/ccxt.md",
    "references/moonwell.md",
    "references/pendle.md",
    "references/boros.md",
    "references/uniswap.md",
    "references/projectx.md",
    "references/tokens-and-pools.md",
    "references/hyperlend.md"
  ],
  "permissions": {
    "network": true,
    "filesystem": true
  }
}
