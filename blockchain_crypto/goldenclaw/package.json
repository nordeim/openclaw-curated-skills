{
  "name": "goldenclaw-skill",
  "version": "1.1.0",
  "description": "OpenClaw skill for GoldenClaw (GCLAW) token (Solana SPL) transactions - exchange services like API tokens and AI compute",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "node test/unit-test.js && node test/onchain-test.js && node test/transaction-test.js",
    "test:unit": "node test/unit-test.js",
    "test:onchain": "node test/onchain-test.js",
    "test:tx": "node test/transaction-test.js",
    "test:e2e": "node test/e2e-transaction-test.js",
    "status": "npx ts-node scripts/check-status.ts",
    "check-claim": "npx ts-node scripts/check-claim.ts",
    "initialize": "npx ts-node scripts/initialize.ts"
  },
  "keywords": [
    "openclaw",
    "solana",
    "spl-token",
    "cryptocurrency",
    "wallet"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.98.4",
    "argon2": "^0.41.0",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "ed25519-hd-key": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
