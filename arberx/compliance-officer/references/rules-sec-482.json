  {
    "id": "SEC-482-c-sales-loads",
    "version": "1.0.0",
    "framework": "sec-482",
    "title": "Sales Load Disclosure",
    "severity": "critical",
    "summary": "Investment company advertisements must disclose maximum sales charges (loads), including both front-end and back-end loads, or clearly state if the fund is no-load.",
    "rationale": "",
    "detection": {
      "type": "hybrid",
      "patterns": [
        "sales\\s+(charge|load)",
        "front[\\s-]?end\\s+load",
        "back[\\s-]?end\\s+load",
        "no[\\s-]?load"
      ],
      "keywords": [
        "sales load",
        "sales charge",
        "no-load",
        "load fund"
      ]
    },
    "remediation": {
      "guidance": "Disclose maximum front-end and back-end sales loads, or clearly state \"no-load\" if applicable.",
      "examples": []
    },
