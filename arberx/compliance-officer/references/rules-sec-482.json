  {
    "id": "SEC-482-b-periods",
    "version": "1.0.0",
    "framework": "sec-482",
    "title": "Required Performance Periods",
    "severity": "critical",
    "summary": "Investment company advertisements showing performance must include 1-year, 5-year, and 10-year (or since inception if shorter) average annual total returns.",
    "rationale": "",
    "detection": {
      "type": "hybrid",
      "patterns": [
        "1[\\s-]?year",
        "5[\\s-]?year",
        "10[\\s-]?year",
        "since\\s+inception"
      ],
      "keywords": [
        "1-year",
        "5-year",
        "10-year",
        "since inception"
      ]
    },
    "remediation": {
      "guidance": "Include 1-year, 5-year, and 10-year (or since inception) average annual total returns.",
      "examples": []
    },
    "source": {
      "source_type": "public_law",
      "policy_status": "allowed",
      "citation": "17 CFR 230.482(b)(1)",
