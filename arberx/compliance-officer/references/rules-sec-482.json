  {
    "id": "SEC-482-b-current-quarter",
    "version": "1.0.0",
    "framework": "sec-482",
    "title": "Most Recent Calendar Quarter",
    "severity": "critical",
    "summary": "Performance data in investment company advertisements must be updated to the most recent calendar quarter-end and include an 'as of' date.",
    "rationale": "",
    "detection": {
      "type": "hybrid",
      "patterns": [
        "as\\s+of\\s+(Q[1-4]|[A-Za-z]+\\s+\\d{1,2},?\\s+\\d{4})",
        "(January|February|March|April|May|June|July|August|September|October|November|December)\\s+\\d{1,2},?\\s+\\d{4}"
      ],
      "keywords": [
        "as of",
        "quarter-end",
        "current performance"
      ]
    },
    "remediation": {
      "guidance": "Update performance data quarterly. Include \"as of\" date for all performance figures.",
      "examples": []
    },
    "source": {
      "source_type": "public_law",
      "policy_status": "allowed",
      "citation": "17 CFR 230.482(b)(2)",
      "source_url": "https://www.ecfr.gov/current/title-17/chapter-II/part-230/section-230.482",
      "retrieved_at": "2026-02-09",
      "attribution_required": false
    },
    "metadata": {
